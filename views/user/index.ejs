  <body>
    <%- include('../includes/navigation.ejs') %>
    <main>
      <h1>Users List :</h1>
      <ul>
        <% user.forEach((student) => { %>
           <li> 
              <p>Name: <%= student.name %></p>
              <p>Nim:<%= student.nim %></p>
              <p>Address:<%= student.address %></p>
              <form action="/user/user/<%= student.id %>" method="POST">
                  <button type="submit">Details Stundent</button>
              </form>
           </li>
        <% }); %> 
       </ul>
    </main>
<%- include('../includes/end.ejs') %>
